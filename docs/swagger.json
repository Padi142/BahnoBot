                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "updates user with incoming json struct",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
